{
	"verbose": true,
	"requireCleanWorkingDir": false,
	"force": true,
	"src": {
		"commitMessage": "Release %s"
	},
	"npm": {
		"publish": true,
		"private": false
	},
	"github": {
		"release": true
	},
	"beforeChangelogCommand": "conventional-changelog -i CHANGELOG.md -s -n .",
	"changelogCommand": "conventional-changelog -n . | tail -n +3",
	"safeBump": false
}
